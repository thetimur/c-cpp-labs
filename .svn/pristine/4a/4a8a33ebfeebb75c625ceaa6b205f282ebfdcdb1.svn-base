all: bin/str.o bin/test_str.o link run_tests

bin/str.o: src/str.c include/str.h
	gcc -c src/str.c -o bin/str.o -I include

bin/test_str.o: src/test_str.c include/test_str.h
	gcc -c src/test_str.c -o bin/test_str.o -I include

bin/test.o: src/test.c
	gcc -c src/test.c -o bin/test.o -I include

link: bin/test.o bin/str.o bin/test_str.o 
	gcc bin/test.o bin/str.o bin/test_str.o -o lab_02 -I include

run_tests:
	./lab_02

clean: 
	rm bin/*.o lab_02